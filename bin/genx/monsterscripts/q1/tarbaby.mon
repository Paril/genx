stand1 $walk1 $walk1
{
	stand
}

walk1 $walk1 $walk25
{
	walk +repeat 11
	^ +speed 2 +repeat 14
}

run1 $run1 $run25
{
	run +repeat 11
	^ +speed 2 +repeat 14
}

fly1 $fly1 $fly4
{
	move +repeat 3
	^ +event tbaby_fly_go
}

jump1 $jump1 $jump6 tbaby_fly
{
	charge +repeat 4
	^ +event tbaby_jump_do
	@0
}

die1 $exp $exp tbaby_explode
{
	move +event tbaby_nohurt
}

